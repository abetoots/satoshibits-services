{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Web apps",
  "extends": "./base.json",
  "compilerOptions": {
    "lib": ["es2022", "DOM", "DOM.Iterable"],
    "module": "NodeNext",
    "moduleDetection": "force",
    "moduleResolution": "Bundler",
    "target": "ES2022",
    "jsx": "react-jsx",
    "noEmit": true
  }
}
