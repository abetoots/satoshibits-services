{
  "name": "@satoshibits/queue",
  "version": "1.0.7",
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": "./dist/index.mjs",
  "type": "module",
  "private": false,
  "scripts": {
    "build": "rm -rf dist && tsc",
    "lint": "pnpm exec eslint .",
    "prepublishOnly": "npm run build",
    "pre-commit": "pnpm exec lint-staged -c ./.lintstagedrc.mjs --concurrent false",
    "test": "vitest --run",
    "test:integration": "docker compose -f docker-compose.test.yml up -d && vitest --run --config vitest.integration.config.mts; docker compose -f docker-compose.test.yml down",
    "test:integration:up": "docker compose -f docker-compose.test.yml up -d",
    "test:integration:down": "docker compose -f docker-compose.test.yml down",
    "test:integration:run": "vitest --run --config vitest.integration.config.mts"
  },
  "files": [
    "./dist/**/*",
    "!./dist/**/*.test.*"
  ],
  "keywords": [
    "queue"
  ],
  "author": {
    "name": "Abe M. Caymo",
    "email": "caymo.abesuni@gmail.com",
    "url": "https://github.com/abetoots"
  },
  "license": "ISC",
  "description": "A queue handler for managing background jobs using worker threads.",
  "devDependencies": {
    "@aws-sdk/client-sqs": "^3.899.0",
    "@eslint/compat": "^1.2.0",
    "@repo/typescript-config": "workspace:^",
    "@satoshibits/eslint-config": "workspace:^",
    "@types/node": "^22.7.4",
    "@typescript-eslint/utils": "^8.26.1",
    "aws-sdk-client-mock": "^4.1.0",
    "bree": "^9.2.4",
    "bullmq": "^5.58.7",
    "eslint": "^9.22.0",
    "express": "^5.1.0",
    "ioredis": "^5.8.0",
    "jiti": "^2.4.2",
    "lint-staged": "^15.2.10",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@satoshibits/functional": "^1.1.0",
    "generic-pool": "^3.9.0"
  },
  "peerDependencies": {
    "tsx": ">= 4.19.0"
  }
}
